{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://your-backend-url.vercel.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}